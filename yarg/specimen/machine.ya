place struct {
    uint32 cpuid;
    uint32 gpio_in;
    uint32 gpio_hi_in;
    struct {
        uint32 rw;
        uint32 set;
        uint32 clr;
        uint32 xor;
        }@0x010 gpio_out;
    struct {
        uint32 rw;
        uint32 set;
        uint32 clr;
        uint32 xor;
        }@0x020 gpio_oe;
    } @xd0000000 sio_hw;

fun coreNum() {
    return peek(sio_hw.cpuid);
}
