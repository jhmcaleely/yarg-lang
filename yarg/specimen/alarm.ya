import("gpio");
import("timer");

gpio_init(pico_led); 
gpio_set_direction(pico_led, true);

var led_state = false; 

fun led() { 
    led_state = !led_state; 
    gpio_put(pico_led, led_state); 
} 

alarm_in_ms(0d0, 500, led);

y_sleep_ms(1000);

repeating_alarm_ms(0d0, 500, led);

y_sleep_ms(3000); 

cancel_repeating_alarm(0d0);