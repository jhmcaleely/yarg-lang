place struct { 
    uint32@10 ctrl;
    uint32@20 flags;
    } @xc0000000 more_registers;
peek(more_registers.ctrl);     // expect: peek(0xc000000a)
peek(more_registers.flags);    // expect: peek(0xc0000014)

place struct { 
    uint32@0d0 << 0d12 rw;
    uint32@0d1 << 0d12 xor;
    uint32@0d2 << 0d12 set;
    uint32@0d3 << 0d12 clr;
    } @xc0000000 fourkregister;
peek(fourkregister.rw);     // expect: peek(0xc0000000)
peek(fourkregister.xor);    // expect: peek(0xc0001000)
peek(fourkregister.set);    // expect: peek(0xc0002000)
peek(fourkregister.clr);    // expect: peek(0xc0003000)

place struct { 
    uint32 begin;
    uint32@0d1 << 0d4 ctrl;
    uint32 ctrl2;
    uint32[2]@0d2 << 0d4 flags;
    uint32 flags2;
    } @xc0000000 registers2;
peek(registers2.ctrl);     // expect: peek(0xc0000010)
peek(registers2.flags);    // expect: peek(0xc0000020)
peek(registers2.begin);    // expect: peek(0xc0000000)
peek(registers2.ctrl2);    // expect: peek(0xc0000014)
peek(registers2.flags2);   // expect: peek(0xc0000028)
