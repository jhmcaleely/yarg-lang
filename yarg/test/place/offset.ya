place struct { 
    muint32@10 ctrl;
    muint32@20 flags;
    } 0xc0000000 more_registers;
rpeek(more_registers.ctrl);     // expect: rpeek(c000000a)
rpeek(more_registers.flags);    // expect: rpeek(c0000014)

place struct { 
    muint32@0d0 << 0d12 rw;
    muint32@0d1 << 0d12 xor;
    muint32@0d2 << 0d12 set;
    muint32@0d3 << 0d12 clr;
    } 0xc0000000 fourkregister;
rpeek(fourkregister.rw);     // expect: rpeek(c0000000)
rpeek(fourkregister.xor);    // expect: rpeek(c0001000)
rpeek(fourkregister.set);    // expect: rpeek(c0002000)
rpeek(fourkregister.clr);    // expect: rpeek(c0003000)

place struct { 
    muint32 begin;
    muint32@0d1 << 0d4 ctrl;
    muint32 ctrl2;
    muint32[2]@0d2 << 0d4 flags;
    muint32 flags2;
    } 0xc0000000 registers2;
rpeek(registers2.ctrl);     // expect: rpeek(c0000010)
rpeek(registers2.flags);    // expect: rpeek(c0000020)
rpeek(registers2.begin);     // expect: rpeek(c0000000)
rpeek(registers2.ctrl2);    // expect: rpeek(c0000014)
rpeek(registers2.flags2);     // expect: rpeek(c0000028)
