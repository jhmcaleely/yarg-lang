var f;
var g;

{
  var string local = "local";
  fun f_() {
    print local;
    local = "after f";
    print local;
  }
  f = f_;

  fun g_() {
    print local;
    local = 10; // expect runtime error: Cannot set local variable to incompatible type.
    print local;
  }
  g = g_;
}

f();
// expect: local
// expect: after f

g();
// expect: after f
