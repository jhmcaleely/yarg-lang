var struct {
    struct {
        int32 x;
        int32 y;
        } loc;
    bool name;
    struct {
        uint32[3] registers;
        end;
        } cpu_registers;
    } location;

print location.name;  // expect: false
print location.loc.x; // expect: 0
print location.loc.y; // expect: 0
location.name = true; // set name to true
print location.name;  // expect: true
location.loc.x = 10; // set x to 10
print location.loc.x; // expect: 10
location.cpu_registers.registers[1] = 0d42; // set first register to 42
print location.cpu_registers.registers[1]; // expect: 42
print location.cpu_registers; // expect: struct{|2:28|Type:uint32[3]:[0, 42, 0]; nil; }
