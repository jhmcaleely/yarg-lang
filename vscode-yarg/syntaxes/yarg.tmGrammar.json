{
    "scopeName": "source.ya",
    "patterns": [
        { "include": "#comments" }, 
        { "include": "#keywords"}, 
        { "include": "#strings" }
    ],
    "repository": {
      "comments": {
        "patterns": [
            { "name": "comment.line.yarg", "match": "\\/\\/.*" }] 
      },
      "strings": {
        "name": "string.yarg",
        "begin": "\"",
        "end": "\"",
        "patterns": []
      },
      "keywords": {
            "name": "keyword.yarg",
            "match": "\\b(and|any|bool|class|const|cpeek|else|false|for|fun|if|import|int8|int16|int32|int64|len|mfloat64|make_channel|make_routine|make_sync_group|new|nil|or|peek|pin|place|poke|print|receive|resume|return|send|set_element|share|start|string|struct|super|this|true|uint8|uint16|uint32|uint64|var|while|yield)\\b"
        }
    }
}
  